{
	"name": "touchbarconsolelog",
	"author": {
		"name": "MrP"
	},
	"displayName": "Touch Bar console.log()",
	"description": "This extension adds a console.log(); to your MacBook Pro's Touch Bar while you are working in VSCode.",
	"publisher": "MrP",
	"galleryBanner": {
		"color": "#2e323b",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/VincentPuget/touchbarconsolelog"
	},
	"version": "1.0.0",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"MacBook pro",
		"VSCode",
		"Touch Bar",
		"console.log()"
	],
	"activationEvents": [
		"onCommand:extension.touchbarconsolelog"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.touchbarconsolelog",
				"title": "console.log();"
			}
		],
		"menus": {
            "touchBar": [
                {
                    "command": "extension.touchbarconsolelog",
					"group": "editing"
                }
            ]
        }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
